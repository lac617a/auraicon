{
  "name": "auraicon",
  "version": "0.0.1",
  "description": "Premium icons for auraicon <3",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/auraicon.js",
  "files": [
    "components/",
    "dist/",
    "icons/"
  ],
  "scripts": {
    "build": "tsc -p scripts/tsconfig.json && npm run build.files && npm run build.component && npm run collection.copy",
    "build.files": "node scripts/build.js",
    "build.component": "stencil build",
    "collection.copy": "node scripts/collection-copy.js",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "release": "np --no-2fa",
    "version": "npm run build"
  },
  "dependencies": {
    "@stencil/core": "^2.13.0"
  },
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "jest": "^27.4.5",
    "jest-cli": "^27.4.5",
    "puppeteer": "^10.0.0",
    "@types/fs-extra": "^9.0.6",
    "@types/node": "^14.14.22",
    "@types/svgo": "^1.3.3",
    "fs-extra": "^9.1.0",
    "np": "^7.2.0",
    "svgo": "1.3.2",
    "typescript": "^4.1.3"
  },
  "keywords": [
    "icon pack",
    "aura",
    "icon",
    "svg",
    "web component",
    "component",
    "custom element"
  ],
  "author": {"name": "yoydev", "url": "https://lac617a.github.io/yoydev.github.io/"},
  "repository": {
    "type": "git",
    "url": "https://github.com/ionic-team/ionicons.git"
  },
  "bugs": {
    "url": "https://github.com/ionic-team/ionicons/issues"
  },
  "license": "MIT"
}
